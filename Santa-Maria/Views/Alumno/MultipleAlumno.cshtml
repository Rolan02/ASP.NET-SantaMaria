@model List<Alumno>
@{
    ViewData["Title"] = "Informacion Alumno";
}
<h1> @ViewData["Title"]</h1>
<strong>Vista de ventana parcial</strong>

<table class="table table-dark table-hover table-sm ">
    <thead>
        <tr>
            <th scope="col">Nombre</th>
            <th scope="col">Id</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var alumno in Model)
        {
        <tr>
            <td>@alumno.Nombre</td>
            <td>@alumno.UniqueId</td>
        </tr>
        }
    </tbody>
</table>

<p><i>@ViewBag.DataDinamico</i></p>
<p><i>@ViewBag.Fecha</i></p>
